<?xml version="1.0" encoding="utf-8" ?>
<ContentPage 
	xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
    prism:ViewModelLocator.AutowireViewModel="True"
	xmlns:local="clr-namespace:kcdnug.Mobile.Views;assembly=kcdnug.Mobile"
    x:Class="kcdnug.Mobile.Views.LoginPage"
    Title="Login">



	  <StackLayout VerticalOptions="Center" HorizontalOptions="Center">
          <Label Text="LoginPage" VerticalOptions="Center" HorizontalOptions="Center" />
		          <Button Text="Main Menu" Command="{Binding Path=NavigateAbsoluteCommand}" CommandParameter="/MainMenuPage/NavigationPage/EventListPage" />
	  </StackLayout>
    

</ContentPage>

